<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bgl"
    tools:context=".RegTutor">

    <Button
        android:id="@+id/head55"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:backgroundTint="#1FECD4"
        android:enabled="false"
        android:fontFamily="@font/audiowide"
        android:includeFontPadding="false"
        android:text="Username"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#0E0D0D"
        android:textSize="14sp"
        app:cornerRadius="@dimen/padQ"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tutUN"
        app:layout_constraintHorizontal_bias="0.513"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/head18"
        app:layout_constraintVertical_bias="0.042" />

    <Button
        android:id="@+id/head58"
        android:layout_width="wrap_content"
        android:layout_height="41dp"
        android:backgroundTint="#1FECD4"
        android:enabled="false"
        android:fontFamily="@font/audiowide"
        android:includeFontPadding="false"
        android:text="Date Of Birth"
        android:textAllCaps="false"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#0E0D0D"
        android:textSize="10sp"
        app:cornerRadius="@dimen/padQ"
        app:layout_constraintBottom_toTopOf="@+id/head59"
        app:layout_constraintEnd_toStartOf="@+id/tutBday"
